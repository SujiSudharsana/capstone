    "import pandas as pd\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.metrics import accuracy_score, classification_report\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.model_selection import cross_val_score\n",
    "\n",
    "# Assuming df_pre is your DataFrame with outliers removed (already loaded)\n",
    "\n",
    "# Step 1: Prepare features (X) and target (y)\n",
    "X = df_pre.drop(columns=['Status_Not Joined'])  # Assuming 'Status_Not Joined' is the target\n",
    "y = df_pre['Status_Not Joined']\n",
    "\n",
    "# Step 2: Convert categorical columns to dummy/one-hot encoding\n",
    "X = pd.get_dummies(X, drop_first=True)  # Drop the first category to avoid multicollinearity\n",
    "\n",
    "# Step 3: Normalize the data (important for certain models like SVM, KNN, etc.)\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "# Step 4: Split the data into training and testing sets\n",
    "X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)\n",
    "\n",
    "# Check the shapes of the data to confirm\n",
    "print(f\"Training data: {X_train.shape}\")\n",
    "print(f\"Testing data: {X_test.shape}\")\n",
    "\n",
    "# Step 5: Build a RandomForestClassifier model\n",
    "rf_model = RandomForestClassifier(n_estimators=100, random_state=42)\n",
    "\n",
    "# Step 6: Train the model\n",
    "rf_model.fit(X_train, y_train)\n",
    "\n",
    "# Step 7: Predict on the test set\n",
    "y_pred = rf_model.predict(X_test)\n",
    "\n",
    "# Step 8: Evaluate the model\n",
    "accuracy = accuracy_score(y_test, y_pred)\n",
    "print(f\"Accuracy: {accuracy:.2f}\")\n",
    "\n",
    "# Additional performance metrics\n",
    "print(\"Classification Report:\\n\", classification_report(y_test, y_pred))\n",
    "\n",
    "# Optional: Perform cross-validation to get more insights into model performance\n",
    "cv_scores = cross_val_score(rf_model, X_scaled, y, cv=5, scoring='accuracy')\n",
    "print(f\"Average Cross-Validation Accuracy: {cv_scores.mean():.2f}\")\n",
    "\n"
   ]
