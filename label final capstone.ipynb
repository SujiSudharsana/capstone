    "import pandas as pd\n",
    "from scipy import stats\n",
    "\n",
    "\n",
    "\n",
    "# Define the Z-score threshold for outlier detection\n",
    "z_score_threshold = 4\n",
    "\n",
    "# Calculate the Z-scores for all columns (or you can select specific columns for this)\n",
    "z_scores = stats.zscore(df.select_dtypes(include=['float64', 'int64']))\n",
    "\n",
    "# Create a DataFrame with the Z-scores\n",
    "z_score_df = pd.DataFrame(z_scores, columns=df.select_dtypes(include=['float64', 'int64']).columns)\n",
    "\n",
    "# Identify the rows where any Z-score exceeds the threshold\n",
    "filter_condition = (z_score_df.abs() > z_score_threshold).any(axis=1)\n",
    "\n",
    "# Create a new DataFrame excluding the outliers\n",
    "df_pre = df[~filter_condition]\n",
    "\n",
    "# Print the number of outliers removed\n",
    "num_outliers_removed = filter_condition.sum()\n",
    "print(f'Number of outliers removed: {num_outliers_removed}')\n",
    "\n",
    "# Print the DataFrame after removing outliers\n",
    "print(df_pre)\n",
    "\n",
    "\n"
